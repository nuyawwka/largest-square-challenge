def largest_square(a, b):
    r = 0
    s = 1
    t = (a + b) // 4
    
    while s <= t:
        m  = (s + t) // 2
        n  = a // m
        n += b // m
        
        if n < 4:
            t = m - 1
        else:
            r = m
            s = m + 1
                    
    return r

n = largest_square(100, 92)
print(n)
