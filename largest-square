def largest_square(a, b):
    r  = 0
    lo = 1
    hi = (a + b) // 4
    
    while lo <= hi:
        m  = lo + ((hi - lo + 1) // 2)
        n  = a // m
        n += b // m
        
        if n < 4:
            hi = m - 1
        else:
            r  = m
            lo = m + 1
                    
    return r

n = largest_square(13, 11)
print(n)
